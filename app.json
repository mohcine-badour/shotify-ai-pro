    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": [
      {
        "asset": "./assets/fonts/Fear-robot-regular.ttf",
        "family": "Fear-robot-regular"
      }
    ],
    "extra": {
      "eas": {
        "projectId": "d65f84ba-12ff-477f-8e6c-49b9c9dc7ed2"
